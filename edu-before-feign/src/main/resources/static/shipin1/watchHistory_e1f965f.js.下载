define("watchHistory",function(i){var t=i("TTP"),e=i("TTR"),o=i("TTA");!function(){function i(){var i="huya_watchHistory",t={historyTime:Math.floor(new Date/1e3),isLive:1,l_p:o.lp,privateHost:o.host,profileRoom:e.profileRoom,yyid:o.yyid,nick:o.nick,gameName:e.gameFullName,source:"web"};!function(e){var o=null;try{o=JSON.parse(e)}catch(s){}var r=null;r=o&&o.historyList?o:{historyList:[]},$.each(r.historyList,function(i,e){return e.l_p==t.l_p?(r.historyList.splice(i,1),!1):void 0}),r.historyList.unshift(t),r.historyList=r.historyList.slice(0,5);var n=JSON.stringify(r);localStorage.setItem(i,n),TT.event.emit("TT_WATCH_HISTORY_UPDATE",n,r)}(localStorage.getItem(i))}NAV_UTIL.isLogin()||e.isOn&&t.ready(function(){setTimeout(i,5e3)})}()});