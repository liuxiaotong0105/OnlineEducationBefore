define("BlockWords",function(t,i,n){function e(t){var i=$.extend(this,new s);i.list=t&&t.length?t:[],i.add=function(t){var n=$.trim(t);"string"==typeof t&&n&&-1==r(i.list,n)&&(i.list.push(n),i.emit("add",n))},i.del=function(t){var n=$.trim(t);if("string"==typeof t&&n){var e=r(i.list,n);-1!==e&&(i.list.splice(e,1),i.emit("del",n))}},i.update=function(t){i.list=t&&t.length?t:[],i.emit("update",i.list)}}function r(t,i){var n=-1;return $.each(t,function(t,e){return e===i?(n=t,!1):void 0}),n}var s=t("Event");n.exports=e});