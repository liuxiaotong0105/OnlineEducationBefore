<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jk.mapper.CeMapper">
	
	<select id="queryCe" resultType="com.jk.modelapi.Ce" >
		select * from t_ce
	</select>
	<insert id="addMovie">

		INSERT into t_movie
		 (movieId,movieName,movieInfo,moviePrice,movieStatus,movieType,movieUrl,moviePicther,teacherid) VALUES
			(#{movieId},#{movieName},#{movieInfo},#{moviePrice},0,#{movieType},#{movieUrl},#{movieUrl},#{teacherid})
	</insert>



</mapper>